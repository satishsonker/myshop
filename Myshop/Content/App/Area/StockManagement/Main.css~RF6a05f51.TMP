.control {
    width: 100%;
    /* border: 1px solid; */
    min-height: 39px;
    padding: 5px;
}

.holder input[type="submit"] {
    margin: 0;
    display: block;
    padding: 8px 0;
    width: 148px;
    font-size: 12px;
    font-weight: bold;
    border: 0;
    text-shadow: 0px 1px 0px rbga(255, 255, 255, 1);
    background-color: #f6ba35;
    background-image: -webkit-linear-gradient(90deg, #eca418, #ffcd4e);
    border-radius: 5px;
    box-shadow: 0px 2px 5px rgba(0, 0, 0, 0.3);
    float: right;
    color: black;
}

