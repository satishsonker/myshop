.holder{
    width: 664px !important;
    background-color: #1ea167;
    border-radius: 5px;
    padding: 21px;
    background-image: -webkit-linear-gradient(90deg, #168d59, #1ea167);
    margin-top: 4%;
    margin-top: 0% !important;;
    margin-left: 20% !important;;
    box-shadow: 0px 1px 0px rgba(255, 255, 255, 0.2) inset, 0px 0px 2px rgba(0, 0, 0, 0.5);
}

.holder fieldset label {
    font-size: 12px;
    margin-right: 57px;
}

.holder fieldset input, select{
    margin:5px 0 !important;
    padding:3px !important;
}

.holder fieldset input {
 outline: none;
    width: 333px;
    height: 11px;
    display: block;
    background: #138050;
    border: 1px solid #0d6b42;
    margin: 0;
    padding: 20px 15px;
    font-size: 13px;
}