
@{
    ViewBag.Title = "StockEntry";
    Layout = "~/Areas/StockManagement/Views/Shared/_Layout.cshtml";
}


<link href="~/Content/App/Area/StockManagement/Main.css" rel="stylesheet" />
<link href="~/Content/App/Home/Login.css" rel="stylesheet" />
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/normalize/5.0.0/normalize.min.css">
@*@using (Html.BeginForm("SetUnit", "Masters", new { area = "StockManagement" }, FormMethod.Post, new { style = "margin-top:60px" }))
    {*@

<form action="/" method="post" style="margin-top:60px" id="unitform">
    <div class="container">
        <div class="holder">
            <h3 class="text-center">Product Stock Entry</h3>
            <fieldset>
                <table id="table" class="table table-striped table-bordered" cellspacing="0" width="100%">
                    <thead>
                        <tr>
                            <th>Category</th>
                            <th>Sub Category</th>
                            <th>Date</th>
                            <th id="btnSelect">Delete</th>
                        </tr>
                    </thead>
                    <tfoot>
                        <tr>
                            <th>Category</th>
                            <th>Description</th>
                            <th>Date</th>
                            <th>Add</th>
                        </tr>
                    </tfoot>
                    <tbody></tbody>
                </table>
            </fieldset>

        </div>
    </div>
    <div class="popup">
        <div><img src="~/Images/Icons/if_Close_Icon_1398919.png" id="popupclose" onclick="utility.popupClose()" title="Close" alt="close" /></div>
        <table id="popuptable" class="table table-striped table-bordered" cellspacing="0" width="100%">
            <thead>
                <tr>
                    <th id="UnitName">Product Unit</th>
                    <th id="Description" title="Product Description">Description</th>
                    <th id="CreatedDate">Date</th>
                    <th id="btnSelect">Select</th>
                </tr>
            </thead>
            <tfoot>
                <tr>
                    <th>Category</th>
                    <th>Description</th>
                    <th>Date</th>
                    <th>Select</th>
                </tr>
            </tfoot>
            <tbody></tbody>
        </table>
    </div>
</form>
@*}*@
