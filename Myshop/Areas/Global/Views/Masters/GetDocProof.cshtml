
@{
    ViewBag.Title = "GetDocProof";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<link href="~/Content/App/Area/StockManagement/Main.css" rel="stylesheet" />
<link href="~/Content/App/Home/Login.css" rel="stylesheet" />
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/normalize/5.0.0/normalize.min.css">
<script src="~/Scripts/App/Area/Global/Master/DocProof.js"></script>
<form action="/" method="post" style="margin-top:60px" id="doctypeform">
    <div class="col-sm-6 col-sm-offset-3">
        <div class="panel panel-info">
            <div class="panel-heading">Bank Account Details</div>
            <div class="panel-body">
                <div class="row">
                    <div class="col-sm-12">
                        <input type="hidden" value="0" id="docproofid" name="docproofid" />
                        <label>Doc Proof Type</label>
                        <select id="docprooftypeid" name="docprooftypeid" disabled="disabled" class="disableCtrl form-control" required="required">
                            <option value="">Select Doc Proof Type</option>
                        </select>
                    </div>
                    <div class="col-sm-12">
                        <label>Document Proof Name</label>
                        <input type="text" required="required" placeholder="Document Proof Name" class="form-control" id="DocProof" name="DocProof" />
                    </div>
                    <div class="col-sm-12">
                        <label>Description</label>
                        <input type="text" class="form-control" placeholder="Enter Description" id="Description" name="Description" />
                    </div>
                </div>
            </div>
            <div class="panel-footer">
                @{
                    string ViewDataName = "GetDocProofJson";
                }
                @Html.Partial("_ButtonGroup", ViewDataName)
            </div>
        </div>
    </div>
    <div class="popup">
        <div><img src="~/Images/Icons/if_Close_Icon_1398919.png" id="popupclose" onclick="utility.popupClose()" title="Close" alt="close" style="float: right;padding-bottom: 5px;" /></div>
        <table id="popuptable" class="table table-striped table-bordered" cellspacing="0" width="100%">
            <thead>
                <tr>
                    <th id="DocProofType">Document Proof Type</th>
                    <th id="DocProof">Document Proof</th>
                    <th id="Description">Description</th>
                    <th id="btnSelect">Select</th>
                </tr>
            </thead>
            <tfoot>
                <tr>
                    <th>Document Proof Type</th>
                    <th>Document Proof</th>
                    <th>Description</th>
                    <th>Select</th>
                </tr>
            </tfoot>
            <tbody></tbody>
        </table>
    </div>
</form>


