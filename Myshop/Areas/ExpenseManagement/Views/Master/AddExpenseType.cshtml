
@{
    ViewBag.Title = "Add Expense Type";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}
<script src="~/Scripts/App/Area/Expense/Master/ExpenseType.js"></script>

<form action="/" method="post" style="margin-top:0px" id="exptypeform">
    <div class="col-sm-10 col-sm-offset-1">
        <div class="panel panel-info">
            <div class="panel-heading">Add Expense Type</div>
            <div class="panel-body">
                <div class="row">
                    <div class="col-sm-12">
                        <label>Expense Type</label>
                        <input type="text" required="required" maxlength="50" class="form-control" placeholder="Enter Expense Account Type" id="exptype" name="ExpType" />
                        <input type="hidden" value="0" id="exptypeid" name="exptypeid" />
                    </div>
                </div>
                <div class="row">
                    <div class="col-sm-12">
                        <label>Description</label>
                        <input type="text" class="form-control" placeholder="Enter Expense Type Description" id="exptypedesc" name="exptypedesc" />
                    </div>
                </div>
            </div>
            <div class="panel-footer">
                @{
                    string ViewDataName = "ExpenseArea.MasterController.GetExpTypeJson";
                }
                @Html.Partial("_ButtonGroup", ViewDataName)
            </div>
        </div>
    </div>
    <div class="popup">
        <div><img src="~/Images/Icons/if_Close_Icon_1398919.png" id="popupclose" onclick="utility.popupClose()" title="Close" alt="close" style="float: right;padding-bottom: 5px;" /></div>
        <table id="popuptable" class="table table-striped table-bordered" cellspacing="0" width="100%">
            <thead>
                <tr>
                    <th id="ExpType">Expense Type</th>
                    <th id="ExpTypeDesc" title="Product Description">Description</th>
                    <th id="CreatedDate">Date</th>
                    <th id="btnSelect">Select</th>
                </tr>
            </thead>
            <tbody></tbody>
        </table>
    </div>
</form>

