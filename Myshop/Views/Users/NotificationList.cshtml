
@{
    ViewBag.Title = "NotificationList";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<link href="~/Content/App/Area/StockManagement/Main.css" rel="stylesheet" />
<link href="~/Content/App/Home/Login.css" rel="stylesheet" />
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/normalize/5.0.0/normalize.min.css">
<script src="~/Scripts/App/Users/NotifyList.js"></script>
<form action="/" method="post" style="margin-top:60px" id="notifyform">
    <div class="col-sm-10 col-sm-offset-1">
        <div class="panel panel-info">
            <div class="panel-heading">Notification List</div>
            <div class="panel-body">
                <table class="table table-bordered table-responsive table-striped" id="tblNotificationList">
                   <thead>
                       <tr>
                           <th class="text-center">Sr. No.</th>
                           <th style="width:80%" class="text-center">Message</th>
                           <th style="width:10%" class="text-center">Action</th>
                       </tr>
                   </thead>
                    <tbody></tbody>
                </table>
            </div>
            <div class="panel-footer">
            </div>
        </div>
    </div>
</form>


