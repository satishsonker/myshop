@using Myshop.App_Start
@{
    ViewBag.Title = "No Shop Mapped";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

<div class="col-lg-8 col-lg-offset-2" style="margin-top:60px;">
    <div class="panel panel-danger">
        <div class="panel-heading">No Shop Mapped</div>
        <div class="panel-body">
            <div class="row">
                <div class="col-lg-3">
                    <img src="~/Images/Icons/404.png" alt="Shop Not Found" />
                </div>
                <div class="col-lg-9">
                    <h2>Shop is not mapped with your account</h2>
                    <hr />
                    <h6>Please contact to your administrator @Utility.GetAppSettingsValue("SupportEmail")</h6>
                </div>

            </div>
        </div>
    </div>
</div>

